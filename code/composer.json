{
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": ">=7.1.0",
    "roave/security-advisories": "dev-master",
    "zendframework/zend-diactoros": "^2.1",
    "zendframework/zend-httphandlerrunner": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.4",
    "symfony/var-dumper": "^4.3"
  },
  "autoload": {
    "psr-4": {
      "Skeleton\\": "src/Skeleton/",
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "tests": "vendor/bin/phpunit --colors=always"
  }
}
